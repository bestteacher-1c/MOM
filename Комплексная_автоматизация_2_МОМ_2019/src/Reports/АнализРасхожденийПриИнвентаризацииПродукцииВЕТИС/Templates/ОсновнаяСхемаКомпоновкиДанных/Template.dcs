		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Шапка.ДокументОснование КАК ДокументОснование,
	Шапка.Ссылка КАК ДокументВЕТИС,
	ТабличнаяЧасть.Продукция КАК Продукция,
	ТабличнаяЧасть.Номенклатура КАК Номенклатура,
	ТабличнаяЧасть.Характеристика КАК Характеристика,
	ТабличнаяЧасть.Серия КАК Серия,
	СУММА(ТабличнаяЧасть.КоличествоИзменение) КАК КоличествоИзменение
ПОМЕСТИТЬ ТоварыДокументовВЕТИС
ИЗ
	Документ.ИнвентаризацияПродукцииВЕТИС.Товары КАК ТабличнаяЧасть
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ИнвентаризацияПродукцииВЕТИС КАК Шапка
		ПО (Шапка.Ссылка = ТабличнаяЧасть.Ссылка)
{ГДЕ
	Шапка.Ссылка.* КАК ДокументВЕТИС,
	Шапка.ДокументОснование.* КАК ДокументОснование}

СГРУППИРОВАТЬ ПО
	Шапка.ДокументОснование,
	Шапка.Ссылка,
	ТабличнаяЧасть.Продукция,
	ТабличнаяЧасть.Номенклатура,
	ТабличнаяЧасть.Характеристика,
	ТабличнаяЧасть.Серия

ИНДЕКСИРОВАТЬ ПО
	ДокументВЕТИС
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Товары.ДокументОснование КАК Ссылка
ПОМЕСТИТЬ ПрикладныеДокументы
ИЗ
	ТоварыДокументовВЕТИС КАК Товары

ИНДЕКСИРОВАТЬ ПО
	Товары.ДокументОснование
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Товары.Продукция КАК Продукция,
	МАКСИМУМ(СоответствиеНоменклатуры.Номенклатура) КАК Номенклатура,
	МАКСИМУМ(СоответствиеНоменклатуры.Характеристика) КАК Характеристика,
	МАКСИМУМ(СоответствиеНоменклатуры.Серия) КАК Серия
ПОМЕСТИТЬ ОднозначноСопоставленныеПозиции
ИЗ
	ТоварыДокументовВЕТИС КАК Товары
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.СоответствиеНоменклатурыВЕТИС КАК СоответствиеНоменклатуры
		ПО (СоответствиеНоменклатуры.Продукция = Товары.Продукция)

СГРУППИРОВАТЬ ПО
	Товары.Продукция

ИМЕЮЩИЕ
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ СоответствиеНоменклатуры.Номенклатура) = 1 И
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ СоответствиеНоменклатуры.Характеристика) = 1 И
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ СоответствиеНоменклатуры.Серия) = 1
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Товары.ДокументОснование КАК ДокументОснование,
	Товары.ДокументВЕТИС КАК ДокументВЕТИС,
	Товары.Продукция КАК Продукция,
	ВЫБОР
		КОГДА Товары.Номенклатура = ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
			ТОГДА ЕСТЬNULL(Т.Номенклатура, ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка))
		ИНАЧЕ Товары.Номенклатура
	КОНЕЦ КАК Номенклатура,
	ВЫБОР
		КОГДА Товары.Номенклатура = ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
			ТОГДА ЕСТЬNULL(Т.Характеристика, ЗНАЧЕНИЕ(Справочник.ХарактеристикиНоменклатуры.ПустаяСсылка))
		ИНАЧЕ Товары.Характеристика
	КОНЕЦ КАК Характеристика,
	ВЫБОР
		КОГДА Товары.Номенклатура = ЗНАЧЕНИЕ(Справочник.Номенклатура.ПустаяСсылка)
			ТОГДА ЕСТЬNULL(Т.Серия, ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка))
		ИНАЧЕ Товары.Серия
	КОНЕЦ КАК Серия,
	Товары.КоличествоИзменение КАК КоличествоИзменение
ПОМЕСТИТЬ ТоварыДокументовВЕТИССопоставлено
ИЗ
	ТоварыДокументовВЕТИС КАК Товары
		ЛЕВОЕ СОЕДИНЕНИЕ ОднозначноСопоставленныеПозиции КАК Т
		ПО (Т.Продукция = Товары.Продукция)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Товары.Ссылка КАК Ссылка,
	Товары.Номенклатура КАК Номенклатура,
	Товары.Характеристика КАК Характеристика,
	Товары.Серия КАК Серия,
	-СУММА(Товары.Количество) КАК КоличествоИзменение
ПОМЕСТИТЬ ТоварыНакладной
ИЗ
	Документ.ВнутреннееПотреблениеТоваров.Товары КАК Товары
ГДЕ
	(Товары.Серия &lt;&gt; ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)
			ИЛИ Товары.СтатусУказанияСерий = 0)
	И Товары.Ссылка В
			(ВЫБРАТЬ
				ПрикладныеДокументы.Ссылка
			ИЗ
				ПрикладныеДокументы КАК ПрикладныеДокументы)

СГРУППИРОВАТЬ ПО
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Серии.Ссылка,
	Серии.Номенклатура,
	Серии.Характеристика,
	Серии.Серия,
	-СУММА(Серии.Количество)
ИЗ
	Документ.ВнутреннееПотреблениеТоваров.Серии КАК Серии
ГДЕ
	Серии.Ссылка В
			(ВЫБРАТЬ
				ПрикладныеДокументы.Ссылка
			ИЗ
				ПрикладныеДокументы КАК ПрикладныеДокументы)

СГРУППИРОВАТЬ ПО
	Серии.Ссылка,
	Серии.Номенклатура,
	Серии.Характеристика,
	Серии.Серия

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия,
	СУММА(Товары.Количество)
ИЗ
	Документ.ПрочееОприходованиеТоваров.Товары КАК Товары
ГДЕ
	(Товары.Серия &lt;&gt; ЗНАЧЕНИЕ(Справочник.СерииНоменклатуры.ПустаяСсылка)
			ИЛИ Товары.СтатусУказанияСерий = 0)
	И Товары.Ссылка В
			(ВЫБРАТЬ
				ПрикладныеДокументы.Ссылка
			ИЗ
				ПрикладныеДокументы КАК ПрикладныеДокументы)

СГРУППИРОВАТЬ ПО
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Серии.Ссылка,
	Серии.Номенклатура,
	Серии.Характеристика,
	Серии.Серия,
	СУММА(Серии.Количество)
ИЗ
	Документ.ПрочееОприходованиеТоваров.Серии КАК Серии
ГДЕ
	Серии.Ссылка В
			(ВЫБРАТЬ
				ПрикладныеДокументы.Ссылка
			ИЗ
				ПрикладныеДокументы КАК ПрикладныеДокументы)

СГРУППИРОВАТЬ ПО
	Серии.Ссылка,
	Серии.Номенклатура,
	Серии.Характеристика,
	Серии.Серия

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия,
	-СУММА(Товары.Количество)
ИЗ
	Документ.СписаниеНедостачТоваров.Товары КАК Товары
ГДЕ
	Товары.Ссылка В
			(ВЫБРАТЬ
				ПрикладныеДокументы.Ссылка
			ИЗ
				ПрикладныеДокументы КАК ПрикладныеДокументы)

СГРУППИРОВАТЬ ПО
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия,
	СУММА(Товары.Количество)
ИЗ
	Документ.ОприходованиеИзлишковТоваров.Товары КАК Товары
ГДЕ
	Товары.Ссылка В
			(ВЫБРАТЬ
				ПрикладныеДокументы.Ссылка
			ИЗ
				ПрикладныеДокументы КАК ПрикладныеДокументы)

СГРУППИРОВАТЬ ПО
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия,
	-СУММА(Товары.Количество)
ИЗ
	Документ.ПересортицаТоваров.Товары КАК Товары
ГДЕ
	Товары.Ссылка В
			(ВЫБРАТЬ
				ПрикладныеДокументы.Ссылка
			ИЗ
				ПрикладныеДокументы КАК ПрикладныеДокументы)

СГРУППИРОВАТЬ ПО
	Товары.Ссылка,
	Товары.Номенклатура,
	Товары.Характеристика,
	Товары.Серия

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Товары.Ссылка,
