// Функция возвращает текст запроса дополнительных временных таблиц,
// необходимых для отражения в регламентированном учете
//
// Возвращаемое значение:
//   Строка - сформированный текст запроса.
//
Функция ТекстЗапросаВТОтраженияВРеглУчете() Экспорт
	
	//++ Локализация
	
	ТекстыЗапроса = Новый Массив;
	
	#Область РасчетыСКлиентамиПоДокументам
	
	ТекстЗапроса =
	"ВЫБРАТЬ
	|	РасчетыПоДокументам.Регистратор КАК Ссылка,
	|	РасчетыПоДокументам.ЗаказКлиента,
	|	ВЫБОР КОГДА РасчетыПоДокументам.Регистратор = РасчетыПоДокументам.ЗаказКлиента ТОГДА NULL ИНАЧЕ РасчетыПоДокументам.ЗаказКлиента.Подразделение КОНЕЦ КАК Подразделение,
	|	ВЫБОР КОГДА РасчетыПоДокументам.Регистратор = РасчетыПоДокументам.ЗаказКлиента ТОГДА NULL ИНАЧЕ РасчетыПоДокументам.ЗаказКлиента.ГруппаФинансовогоУчета КОНЕЦ КАК ГруппаФинансовогоУчета,
	|	РасчетыПоДокументам.АналитикаУчетаПоПартнерам.НаправлениеДеятельности КАК НаправлениеДеятельности,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.Предоплата) КАК Предоплата,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.ПредоплатаУпр) КАК ПредоплатаУпр,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.ПредоплатаРегл) КАК ПредоплатаРегл,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.Долг) КАК Долг,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.ДолгУпр) КАК ДолгУпр,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.ДолгРегл) КАК ДолгРегл,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.ЗалогЗаТару) КАК ЗалогЗаТару,
	|	-СУММА(ВЫБОР КОГДА РасчетыПоДокументам.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход) ТОГДА 1 ИНАЧЕ -1 КОНЕЦ * РасчетыПоДокументам.ЗалогЗаТаруРегл) КАК ЗалогЗаТаруРегл
	|ПОМЕСТИТЬ РасчетыСКлиентамиПоДокументам
	|ИЗ
	|	ДокументыКОтражению КАК ДокументыКОтражению
	|	ВНУТРЕННЕЕ СОЕДИНЕНИЕ
	|		РегистрНакопления.РасчетыСКлиентамиПоДокументам КАК РасчетыПоДокументам
	|	ПО
	|		ДокументыКОтражению.Ссылка = РасчетыПоДокументам.Регистратор
	|		И НЕ &НоваяАрхитектураВзаиморасчетов
	|ГДЕ
	|	РасчетыПоДокументам.ХозяйственнаяОперация В (ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.УдержаниеВознагражденияКомиссионера),
	|										ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ЗачетВознагражденияАвансомКомиссионера),
	|										ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ЗачетВознагражденияОплатойКомиссионера))
	|	
	|СГРУППИРОВАТЬ ПО
	|	РасчетыПоДокументам.ЗаказКлиента,
	|	РасчетыПоДокументам.Регистратор,
	|	ВЫБОР КОГДА РасчетыПоДокументам.Регистратор = РасчетыПоДокументам.ЗаказКлиента ТОГДА NULL ИНАЧЕ РасчетыПоДокументам.ЗаказКлиента.Подразделение КОНЕЦ,
	|	ВЫБОР КОГДА РасчетыПоДокументам.Регистратор = РасчетыПоДокументам.ЗаказКлиента ТОГДА NULL ИНАЧЕ РасчетыПоДокументам.ЗаказКлиента.ГруппаФинансовогоУчета КОНЕЦ,
	|	РасчетыПоДокументам.АналитикаУчетаПоПартнерам.НаправлениеДеятельности";
	
	ТекстыЗапросаПоДокументам = Новый Массив;
	ТекстыЗапросаПоДокументам.Добавить(ТекстЗапроса);
	
	ТекстЗапроса =
	"ВЫБРАТЬ
	|	Расчеты.Ссылка КАК Ссылка,
	|	Расчеты.ОбъектРасчетов КАК ЗаказКлиента,
	|	Расчеты.Подразделение КАК Подразделение,
	|	Расчеты.ГруппаФинансовогоУчета КАК ГруппаФинансовогоУчета,
	|	Расчеты.НаправлениеДеятельности КАК НаправлениеДеятельности,
	|	СУММА(Расчеты.Предоплата) КАК Предоплата,
	|	СУММА(Расчеты.ПредоплатаУпр) КАК ПредоплатаУпр,
	|	СУММА(Расчеты.ПредоплатаРегл) КАК ПредоплатаРегл,
	|	СУММА(-Расчеты.Долг) КАК Долг,
	|	СУММА(-Расчеты.ДолгУпр) КАК ДолгУпр,
	|	СУММА(-Расчеты.ДолгРегл) КАК ДолгРегл,
	|	СУММА(-Расчеты.ЗалогЗаТару) КАК ЗалогЗаТару,
	|	СУММА(-Расчеты.ЗалогЗаТаруРегл) КАК ЗалогЗаТаруРегл
	|ИЗ
	|	РасчетыСКлиентамиНоваяАрхитектура КАК Расчеты
	|
	|ГДЕ
	|	Расчеты.ХозяйственнаяОперация В (ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.УдержаниеВознагражденияКомиссионера),
	|										ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ЗачетВознагражденияАвансомКомиссионера),
	|										ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ЗачетВознагражденияОплатойКомиссионера))
	|
	|СГРУППИРОВАТЬ ПО
	|	Расчеты.Ссылка,
	|	Расчеты.ОбъектРасчетов,
	|	Расчеты.Подразделение,
	|	Расчеты.ГруппаФинансовогоУчета,
	|	Расчеты.НаправлениеДеятельности";
	
	ТекстыЗапросаПоДокументам.Добавить(ТекстЗапроса);
	ТекстЗапроса = СтрСоединить(ТекстыЗапросаПоДокументам, ОбщегоНазначенияУТ.РазделительЗапросовВОбъединении());
	ТекстЗапроса = ТекстЗапроса + "
	|	
	|ИНДЕКСИРОВАТЬ ПО
	|	Ссылка";
	ТекстыЗапроса.Добавить(ТекстЗапроса);
	
	#КонецОбласти
	
	#Область РасчетыПоВознаграждению
	
	ТекстЗапроса =
	"ВЫБРАТЬ
	|	ДокументыКОтражению.Ссылка КАК Ссылка,
	|	ВЫБОР
	|		КОГДА ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику, РасчетыСКлиентами.ЗаказКлиента) = ДокументыКОтражению.Ссылка
	|		ТОГДА NULL
	|		ИНАЧЕ ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику.Подразделение, РасчетыСКлиентами.ЗаказКлиента.Подразделение)
	|	КОНЕЦ КАК Подразделение,
	|	ВЫБОР
	|		КОГДА ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику, РасчетыСКлиентами.ЗаказКлиента) = ДокументыКОтражению.Ссылка
	|		ТОГДА NULL
	|		ИНАЧЕ ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику.ГруппаФинансовогоУчета, РасчетыСКлиентами.ЗаказКлиента.ГруппаФинансовогоУчета)
	|	КОНЕЦ КАК ГруппаФинансовогоУчета,
	|	ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику, РасчетыСКлиентами.ЗаказКлиента) КАК ОбъектРасчетов,
	|	ЕСТЬNULL(РасчетыСПоставщиками.АналитикаУчетаПоПартнерам.НаправлениеДеятельности, РасчетыСКлиентами.АналитикаУчетаПоПартнерам.НаправлениеДеятельности) КАК НаправлениеДеятельности,
	|	СУММА(ЕСТЬNULL(РасчетыСПоставщиками.Сумма, РасчетыСКлиентами.Сумма)) КАК Сумма,
	|	СУММА(ЕСТЬNULL(РасчетыСПоставщиками.СуммаРегл, РасчетыСКлиентами.СуммаРегл)) КАК СуммаРегл,
	|	СУММА(ЕСТЬNULL(РасчетыСПоставщиками.СуммаУпр, РасчетыСКлиентами.СуммаУпр)) КАК СуммаУпр
	|ПОМЕСТИТЬ РасчетыПоВознаграждению
	|ИЗ
	|	ДокументыКОтражению КАК ДокументыКОтражению
	|	ЛЕВОЕ СОЕДИНЕНИЕ
	|		РегистрНакопления.РасчетыСКлиентами КАК РасчетыСКлиентами
	|	ПО
	|		ДокументыКОтражению.Ссылка = РасчетыСКлиентами.Регистратор
	|		И РасчетыСКлиентами.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.УдержаниеВознагражденияКомиссионера)
	|	ЛЕВОЕ СОЕДИНЕНИЕ
	|		РегистрНакопления.РасчетыСПоставщиками КАК РасчетыСПоставщиками
	|	ПО
	|		ДокументыКОтражению.Ссылка = РасчетыСПоставщиками.Регистратор
	|		И РасчетыСПоставщиками.ХозяйственнаяОперация  = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ОтчетКомиссионера)
	|
	|СГРУППИРОВАТЬ ПО
	|	ДокументыКОтражению.Ссылка,
	|	ВЫБОР
	|		КОГДА ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику, РасчетыСКлиентами.ЗаказКлиента) = ДокументыКОтражению.Ссылка
	|		ТОГДА NULL
	|		ИНАЧЕ ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику.Подразделение, РасчетыСКлиентами.ЗаказКлиента.Подразделение)
	|	КОНЕЦ,
	|	ВЫБОР
	|		КОГДА ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику, РасчетыСКлиентами.ЗаказКлиента) = ДокументыКОтражению.Ссылка
	|		ТОГДА NULL
	|		ИНАЧЕ ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику.ГруппаФинансовогоУчета, РасчетыСКлиентами.ЗаказКлиента.ГруппаФинансовогоУчета)
	|	КОНЕЦ,
	|	ЕСТЬNULL(РасчетыСПоставщиками.ЗаказПоставщику, РасчетыСКлиентами.ЗаказКлиента),
	|	ЕСТЬNULL(РасчетыСПоставщиками.АналитикаУчетаПоПартнерам.НаправлениеДеятельности, РасчетыСКлиентами.АналитикаУчетаПоПартнерам.НаправлениеДеятельности)
	|	
	|ИНДЕКСИРОВАТЬ ПО
	|	Ссылка,
	|	ОбъектРасчетов";
	
	ТекстыЗапроса.Добавить(ТекстЗапроса);
	
	#КонецОбласти
	
	ТекстЗапроса = СтрСоединить(ТекстыЗапроса, ОбщегоНазначения.РазделительПакетаЗапросов());
	ТекстЗапроса = ТекстЗапроса + ОбщегоНазначения.РазделительПакетаЗапросов();
	
	Возврат ТекстЗапроса;
	
	//-- Локализация
	Возврат "";
	
КонецФункции

#КонецОбласти
